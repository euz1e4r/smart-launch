<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Smart Launch Context Web Application</display-name>
  <servlet>
  	<description>Smart Launch Context Servlet</description>
  	<servlet-class>com.knowmed.auth.SmartLaunchContextServlet</servlet-class>
  	<servlet-name>SmartLaunchServlet</servlet-name>
  </servlet>
  	<servlet-mapping>
    	<servlet-name>SmartLaunchServlet</servlet-name>
		<url-pattern>/_services/smart/Launch/*</url-pattern>
	</servlet-mapping>
</web-app>